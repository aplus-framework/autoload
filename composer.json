{
    "name": "aplus/autoload",
    "type": "library",
    "description": "Aplus Framework Autoload Library",
    "keywords": [
        "autoload",
        "autoloader",
        "loader",
        "locator",
        "file-locator",
        "class-locator"
    ],
    "homepage": "https://aplus-framework.com/docs/autoload",
    "license": "MIT",
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.9",
        "ergebnis/composer-normalize": "^2.13",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Framework\\Autoload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Autoload\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "email": "natanfelles@gmail.com",
        "issues": "https://gitlab.com/aplus-framework/libraries/autoload/issues",
        "wiki": "https://gitlab.com/aplus-framework/libraries/autoload/wikis/home",
        "source": "https://gitlab.com/aplus-framework/libraries/autoload",
        "docs": "https://aplus-framework.gitlab.io/aplus-framework/libraries/autoload/docs/"
    }
}
